{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "KoalaVault",
  "description": "Secure marketplace for AI models with blockchain-based payments. Model creators can monetize their work while buyers securely deploy and run encrypted models.",
  "colors": {
    "primary": "#2563eb",
    "light": "#3b82f6",
    "dark": "#1e40af"
  },
  "background": {
    "color": {
      "light": "#f8fafc",
      "dark": "#111827"
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg",
    "href": "https://www.koalavault.ai"
  },
  "favicon": "/favicon.svg",
  "appearance": {
    "default": "system",
    "strict": false
  },
  "styling": {
    "codeblocks": "system"
  },
  "navbar": {
    "links": [
      {
        "label": "GitHub",
        "href": "https://github.com/koalavault-ai/koalavault.ai",
        "icon": "github",
        "iconType": "brands"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Explore Models",
      "href": "https://www.koalavault.ai/models"
    }
  },
  "banner": {
    "content": "ðŸš€ KoalaVault is now live! Secure marketplace for AI models with blockchain-based payments.",
    "dismissible": true
  },
  "interaction": {
    "drilldown": true
  },
  "navigation": {
    "global": {
      "anchors": [
        {
          "anchor": "Back to Home",
          "icon": "house",
          "href": "https://www.koalavault.ai"
        }
      ]
    },
    "groups": [
      {
        "group": "Getting Started",
        "pages": [
          "getting-started/index",
          {
            "group": "Quick Start",
            "pages": [
              "getting-started/quick-start",
              "getting-started/publish-models",
              "getting-started/deploy-models"
            ]
          }
        ]
      },
      {
        "group": "Koava CLI",
        "pages": [
          "koava/index",
          {
            "group": "Authentication & Configuration",
            "pages": [
              "koava/login",
              "koava/logout",
              "koava/status",
              "koava/config"
            ]
          },
          {
            "group": "Publishing Models",
            "pages": [
              "koava/encrypt",
              "koava/restore",
              "koava/upload",
              "koava/push"
            ]
          },
          {
            "group": "Model Management",
            "pages": [
              "koava/create",
              "koava/list",
              "koava/remove"
            ]
          }
        ]
      },
      {
        "group": "vLLM Deployment",
        "pages": [
          "deploy/docker-run-guide"
        ]
      },
      {
        "group": "Security & Policies",
        "pages": [
          "security/architecture",
          "security/cryptotensors",
          "koava/huggingface-compliance",
          "security/trust-remote-code"
        ]
      },
      {
        "group": "Cryptocurrency",
        "pages": [
          "crypto/wallet-setup",
          "crypto/payment-guide"
        ]
      }
    ]
  },
  "footer": {
    "socials": {
      "website": "https://www.koalavault.ai",
      "github": "https://github.com/koalavault-ai/koalavault.ai"
    }
  },
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true,
    "raiseIssue": true
  },
  "search": {
    "prompt": "Search KoalaVault docs..."
  },
  "integrations": {
    "plausible": {
      "domain": "docs.koalavault.ai"
    }
  },
  "contextual": {
    "options": ["copy"]
  }
}